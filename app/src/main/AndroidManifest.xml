<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mad.e_librarymanager">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ELibraryManager">
        <activity android:name=".user.ActivityCart"></activity>
        <activity android:name=".user.User_Main" />
        <activity android:name=".admin.Admin_View_Books" />
        <activity android:name=".admin.Admin_Update_Book" />
        <activity android:name=".admin.Admin_Add_Book" />
        <activity
            android:name=".AdminDashboard"
            android:label="@string/title_activity_admin_dashboard"
            android:theme="@style/Theme.ELibraryManager.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>